# OpenAI API Key for embeddings and chat
OPENAI_API_KEY=sk-your-openai-api-key

# Qdrant Configuration
# Set QDRANT_USE_MEMORY=true for in-memory storage (recommended for Render free tier)
# Set QDRANT_USE_MEMORY=false and configure URL/KEY for Qdrant Cloud
QDRANT_USE_MEMORY=true
QDRANT_URL=
QDRANT_API_KEY=

# Collection name for book content
QDRANT_COLLECTION=physical-ai-textbook

# CORS origins (comma-separated)
CORS_ORIGINS=http://localhost:3000,https://enggqasim.github.io
